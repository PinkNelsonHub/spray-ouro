{
  "name": "mhwk/spray-ouro",
  "description": "EventStore client for PHP",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.0",
    "guzzlehttp/guzzle": "~6.0",
    "beberlei/assert": "~2.5",
    "ramsey/uuid": ">=2.4",
    "krakjoe/pthreads-polyfill": "~1.1",
    "icicleio/icicle": "dev-master",
    "icicleio/stream": "dev-master",
    "icicleio/concurrent": "dev-master",
    "icicleio/http": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.4"
  },
  "autoload": {
    "psr-4": {
      "Spray\\Ouro\\": "./src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mhwk\\Ouro\\": "./test/"
    }
  }
}
